{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": [
      "error"
    ],
    "import/prefer-default-export": "off",
    "no-console": "off"
  }
}